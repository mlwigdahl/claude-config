{
  "name": "@claude-config/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for Claude Configuration Manager",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "claude",
    "configuration",
    "shared",
    "types"
  ],
  "author": "Matt Wigdahl <mlwigdahl@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "cross-env": "^7.0.3",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}